unset LC_ALL
export LANG=us.UTF-8
export LC_CTYPE=en_US.utf8
export LC_COLLATE=C
export LC_TIME=en_US.utf8
export LC_NUMERIC=en_US.utf8
export LC_MONETARY=en_US.utf8
export LC_MESSAGES=en_US.utf8
export LC_PAPER=en_US.utf8
export LC_NAME=en_US.utf8
export LC_ADDRESS=en_US.utf8
export LC_TELEPHONE=en_US.utf8
export LC_MEASUREMENT=en_US.utf8
export LC_IDENTIFICATION=en_US.utf8

export ACK_COLOR_FILENAME=underline
export CVS_RSH=ssh
export EDITOR=/usr/bin/vim
export FULLNAME='Randy J. Ray'
export HOST=$(hostname)
export JAVA_HOME=/opt/java
export LD_LIBRARY_PATH=/opt/java/jre/lib/i386/client:/opt/java/jre/lib/i386/native_threads:/opt/java/jre/lib/i386:/usr/local/lib:.
export LESS="-MdqC"
export LESSCHARSET=utf-8
export MANPATH=/usr/man:/usr/local/man:/usr/share/man:${HOME}/bin/man:/opt/java/man:{$MANPATH}
export NAME="Randy J. Ray"
export PAGER=/usr/bin/less
export PERL5DB="BEGIN { use Data::Dumper; require 'perl5db.pl'; }"
export SVN_EDITOR=vi
